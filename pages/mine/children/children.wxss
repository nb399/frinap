.container{
  position:relative;
  flex:1;
  display:flex;
  flex-direction:column;
  min-height:100%;
  /* padding-bottom:100rpx; */
  box-sizing:border-box;
  padding: 20rpx;
  padding-bottom: 97rpx;
  font-size: 24rpx;
}
.button-hover{
  background-image: linear-gradient(to left,#f1898a,#f17ea4);
}
.info_block{
  max-width: 750rpx;
  padding: 20rpx;
  padding-top: 0rpx;
  background: #fff;
  margin-bottom: 20rpx;
  box-shadow: 0rpx 0rpx 7rpx 2rpx #cccccc;
  border-radius: 15rpx;
  font-size:28rpx;
  color:#808080;
}
.profile{
  height: 227rpx;
}
.extra{

  position: relative;
}
.editBtn{

  color:#f25d8e;
}
.basic{
  height: 1817rpx;
}
.basic1{
  height: 1507rpx;

}
/* 第一部分开始 */
.header{
  display: flex;
  flex-direction: row;
}
.user-info {
  width: 100%;  
  padding: 17rpx 0 27rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  align-items: center;
  position: relative;
  background-color: #fff;
}
.info{
  margin-left: 20rpx;
  width: 100%;
  margin-right: 50rpx;
}
.info view{
  text-align: center;
}
.info .arrow{
  width: 28rpx;
  height: 28rpx;
}
.info_title{
  font-size: 29rpx;
  color: #3e3a66;
  display: flex;
  flex-direction: row;
  margin-top: 15rpx;
  border-bottom: 1rpx solid #eee;
  padding-bottom: 15rpx;
}
.info_title .line{
  width:10rpx;
  height:39rpx;
  border-left:7rpx solid #f25d8e;
}
.info-body{
  margin-top: 15rpx;
  margin-bottom:100rpx;

}
.info-body .info-sign{
    height: auto;
  margin: auto auto;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2
}

.spread-btn {
  color: #f25d8e;
  margin-top: 14rpx;
  text-align: right;
  display: block;
}
.album{
  position: absolute;
  bottom: 7rpx;
  display: flex;
  flex-direction: row;
}

.albumItem-scroll{
  width: 517rpx;
  height: 97rpx;
  white-space: nowrap;
  margin-left: 7rpx;
}
.albumItem-scroll view{
  display: inline-block;
  width: 97rpx;
  height: 97rpx;
  overflow: hidden;
  border-radius: 50%;
  margin-right: 7rpx;
}
.albumItem-scroll image{
  width: 100%;
  height: 100%;
  /* filter: blur(2px); */
} 


.albumItem-static view{
  display: inline-block;
  width: 97rpx;
  height: 97rpx;
  overflow: hidden;
  border-radius: 50%;
  margin-right: 7rpx;
}
.albumItem-static image{
  width: 97rpx;
  height: 97rpx;
  /* filter: blur(2px); */
} 
.moreBtn{
  display: flex;
  flex-direction: column;
  background-color:#ffe7f2;
  border-radius: 50%;
  width:97rpx;
  height:97rpx;
  color: #f25d8e;
  text-align: center;
  line-height: 44rpx;
}
.moreBtn view{
  height:30rpx;
  margin-left:5rpx;
  position:relative;
  top:13rpx;
}
.label{
  position: relative;
  display: flex;
  align-items: center;
}
.item1{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-bottom: 40rpx;
}
.item1 > .label{
  border:2rpx solid #7f7cf7;
  padding: 7rpx 20rpx 7rpx 20rpx;
  border-radius: 35rpx;
}
.uname{
  background-color: #7f7cf7;
  color: #fff; 
  display: flex;
  align-items: center;
}
.item1 image{
  width: 48rpx;
  height: 48rpx;
}
.item2{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  color: #7f7cf7;
}
.item2 > .label{
  display: flex;
  align-items: center;
  border:2rpx solid #7f7cf7;
  padding: 7rpx 10rpx 7rpx 20rpx;
  border-radius: 35rpx;
}
.item2 image{
  width: 28rpx;
  height: 28rpx;
}

.item1 .item-right{
  position: relative;
  top: -10rpx;
}

.item2 .item-right{
  position: relative;
  top: 10rpx;
}
.userHead {
  width: 310rpx;
  height: 200rpx;
  position: relative;
}
.userHead .avatarBox{
  width: 177rpx;
  height: 177rpx;
  overflow: hidden;
  border-radius: 50%;
}
.userHead .avatar{
  width: 100%;
  height: 100%;
}
.userHead .identify{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top:-3rpx;
  left:3rpx;
}
/* 第一部分结束 */

.inner{
  height: 100%;
}
.tab{
  display: flex;
  flex-direction: row;
  border-bottom: 1rpx solid #eee;
  padding-bottom: 20rpx;
  font-size: 28rpx;
}
.tab .item{
  text-align:center;
  color:#3e3a66;
  width: 50%;
}
.tab .item view{
  padding-bottom:20rpx;
  position:relative;
  bottom:-20rpx;
  width:77%;
  margin:0 auto;
}
.active{
  border-bottom:2rpx solid #f25d8e;
  font-weight: bold;
  color: #f25d8e;
}
swiper{
  width: 100%;
  height: 1747rpx;
}
/* 选项 */
.mySetting {
  width: 100%;
  box-sizing: border-box;
  font-size:28rpx;
  color:#808080;
}

.mySetting .mySetting-in {
  width: 100%;
  background-color: #fff;
  box-sizing: border-box;
}

.option {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 100rpx;
  border-bottom: 1px solid #eee;
}
/* .mine:last-child {
  border: none;
} */
.option-in {
  display: flex;
  align-items: center;
  height: 77rpx;
}
.option-in input{
  width: 277rpx;
  text-align: right;
}
.tubiao {
  width: 20rpx;
  height: 42rpx;
}
.arrow {
  width: 28rpx;
  height: 28rpx;
  margin-left: 7rpx;
} 
.submitBtn .button{
  width: 100%;
  font-size: 37rpx;
  text-align: center;
  line-height: 97rpx;
  height: 97rpx;
  color: #fff;
  position: fixed;
  bottom: 0;
  border-radius: 0;
  background-image: linear-gradient(to left,#f87e7f,#f25d8e);
}


/* 展开收起动画 */
.s{
    flex: 1;
    transition: all 500ms ease-in-out;
}
.tanslate0{
    transform:translate(0,0);
}
.tanslate1{
    transform:translate(0,-100vh);
}